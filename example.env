# Discord Bot Configuration
# Get your token from https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here

# Agent Configuration
# Get channel ID by right-clicking channel with Developer Mode enabled
AGENT_CHANNEL_ID=your_discord_channel_id_here

# Development Logging
# Create a webhook in your Discord server settings
DEVELOPMENT_LOG_WEBHOOK_URL=your_discord_webhook_url_here

# Database Configuration (Optional)
DATABASE_URL=postgresql://user:password@localhost:5432/swarm

# Redis Configuration (Optional)
REDIS_URL=redis://localhost:6379/0

# Logging Configuration
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Agent Settings
MAX_AGENTS=10
AGENT_TIMEOUT=300  # seconds
TASK_TIMEOUT=60    # seconds

# Security Settings
ENCRYPTION_KEY=your_encryption_key_here  # 32 bytes for AES-256
API_KEY=your_api_key_here

# Example values (DO NOT USE IN PRODUCTION):
# DISCORD_TOKEN=MTxxxxxxxxxxxxxxxxxx.xxxxxx.xxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# AGENT_CHANNEL_ID=123456789012345678
# DEVELOPMENT_LOG_WEBHOOK_URL=https://discord.com/api/webhooks/1234567890/abcdefghijklmnopqrstuvwxyz
# DATABASE_URL=postgresql://swarm:swarm123@localhost:5432/swarm
# REDIS_URL=redis://localhost:6379/0
# LOG_LEVEL=INFO
# MAX_AGENTS=10
# AGENT_TIMEOUT=300
# TASK_TIMEOUT=60
# ENCRYPTION_KEY=0123456789abcdef0123456789abcdef
# API_KEY=swarm_api_key_123 